{
  "name": "jokem-gym-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "doctor": "tsx scripts/template-doctor.ts",
    "dev": "npm run doctor && next dev",
    "build": "npm run doctor && next build",
    "tenant:build": "tsx scripts/tenant-build.ts --file tenant.setup.json",
    "tenant:seed:print": "node -e \"const fs=require('fs'); const p='./supabase/seed'; if(!fs.existsSync(p)) {console.log('No existe',p); process.exit(0);} const f=fs.readdirSync(p).filter(x=>x.startsWith('seed.')&&x.endsWith('.sql')).sort(); console.log(f.length?('Seeds:\\n- '+f.join('\\n- ')):'No hay seeds generados a√∫n');\"",
    "deploy:tenant": "npm run tenant:build && npx supabase db push && npm run tenant:seed:run",
    "tenant:seed:run": "tsx scripts/run-seed.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "15.1.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.50.0",
    "recharts": "^2.15.0",
    "tailwind-merge": "^2.5.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "4.1.18",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}